@import "/static/css/vendor.wxss";

page {
  background-color: #FAFAFA;
}
.home-wrapper {
  min-height: 100vh;
}
.home-header {
  padding: 10rpx 0 80rpx;
  color: #FFF;
  text-align: center;
  font-size: 34rpx;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100%;
}
.home-header .user-info {
  height: 180rpx;
}
.home-header .avatar {
  width: 130rpx;
  height: 130rpx;
  margin: 0 auto 20rpx;
}
.home-learn {
  display: flex;
  justify-content: space-around;
  margin-top: 80rpx;
  font-size: 26rpx;
}
.home-learn ._strong {
  display: block;
  margin-bottom: 18rpx;
  font-size: 40rpx;
}
.home-learn-item {
  flex: 1;
}
.create-daka-btn {
  width: 400rpx;
  position: absolute;
  top: 700rpx;
  left: 50%;
  border-radius: 50rpx;
  font-size: 36rpx;
  line-height: 100rpx;
  background-color: #22CDCB;
  transform: translateX(-50%);
}
.create-daka-btn::before {
  content: "+";
  margin-right: 20rpx;
  font-size: 40rpx;
}
.create-daka-btn.dakaed::before {
  display: none;
}
.daka-list-wrapper {
  margin-top: 60rpx;
  padding: 0 36rpx;
  color: #AAA;
  font-size: 26rpx;
}
.daka-list-wrapper ._header {
  display: flex;
  justify-content: space-between;
}
.daka-list-wrapper ._h1 {
  color: #333;
  font-size: 36rpx;
  font-weight: bold;
}
.daka-list-wrapper .created-daka-btn {
  width: 200rpx;
  line-height: 56rpx;
  color: inherit;
  border-radius: 28rpx;
  border: 4rpx solid #E6E6E6;
}
.daka-list-wrapper ._ul {
  margin-top: 40rpx;
  overflow: hidden;
}
.navigator.data-v-4858ecc7 {
  display: flex;
  width: 100%;
  position: fixed;
  bottom: 0;
  line-height: 120rpx;
  background-color: #FFF;
  box-shadow: 0 0 20rpx rgba(0, 0, 0, .1);
}
.navigator ._div.data-v-4858ecc7,
.navigator ._a.data-v-4858ecc7 {
  flex: 1;
  color: #22CDCB;
  text-align: center;
  font-size: 32rpx;
  font-weight: bold;
}
.navigator ._div.data-v-4858ecc7::before,
.navigator ._a.data-v-4858ecc7::before {
  content: "";
  display: inline-block;
  margin-right: 18rpx;
  width: 42rpx;
  height: 44rpx;
  vertical-align: middle;
  background-repeat: no-repeat;
  background-size: 100%;
}
.daka-icon.data-v-4858ecc7::before {
  background-image: url(data:image/png;base64,);
}
.discover-icon.data-v-4858ecc7::before {
  background-image: url(data:image/png;base64,);
}