        <view class="_div data-v-cbad0830 btn daka-btn disabled" wx:if="{{isDaKa}}">已打卡</view>
        <view class="_div data-v-cbad0830 btn daka-btn" bindtap="handleProxy" wx:else data-eventid="{{'1'}}" data-comkey="{{$k}}">打卡</view>
      </block>
    </block>
    <block wx:else>
      <view class="_div data-v-cbad0830 btn daka-btn" bindtap="handleProxy" data-eventid="{{'2'}}" data-comkey="{{$k}}">打卡</view>
    </block>
    <view class="_div data-v-cbad0830 detail-intro">
      <view class="_p data-v-cbad0830 {{[isShrink && isLongIntro ? 'line-overflow' : '']}}">{{intro}}</view>
      <view class="_div data-v-cbad0830 btn" bindtap="handleProxy" wx:if="{{isShrink && isLongIntro}}" data-eventid="{{'3'}}" data-comkey="{{$k}}">展开</view>
    </view>
    <button open-type="share" class="_button data-v-cbad0830"></button>
    <view class="_div data-v-cbad0830 btn join-btn" wx:if="{{! isJoin}}" bindtap="handleProxy" data-eventid="{{'4'}}" data-comkey="{{$k}}">加入该小组</view>
    <template bindcancel="handleProxy" wx:if="{{shareModalStatus}}" data-eventid="{{'5'}}" data-comkey="{{$k}}" data="{{...$root[$kk+'4'], $root}}" is="action-sheet$a5ea58d2"></template>
  </view>
