<template name="index$894595b4">
  <view class="_div data-v-cbad0830 detail-wrapper">
    <view class="_header data-v-cbad0830 detail-header">
      <view class="_p data-v-cbad0830">已坚持打卡（天）</view>
      <view class="_strong data-v-cbad0830">{{day}}</view>
