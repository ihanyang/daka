<template name="daka-item$5b465b8e">
  <view bindtap="handleProxy" data-eventid="{{'0'}}" data-comkey="{{$k}}" class="_li data-v-f5666d06">
    <image class="_img data-v-f5666d06 figure" mode="aspectFill" src="{{item.Cover}}"></image>
    <view class="_div data-v-f5666d06 daka-info">
      <view class="_h2 data-v-f5666d06">{{item.PlanName}}</view>
      <view class="_p data-v-f5666d06">
        <image class="_img data-v-f5666d06 avatar" src="{{item.Avatar}}" wx:key="item" key="{{item}}" wx:for="{{item.AvatarList}}" wx:for-index="index" wx:for-item="item"></image>
        <label class="_span data-v-f5666d06">今天已有{{item.TodayClockNum || item.ClockNum}}人打卡</label>
      </view>
    </view>
    <block wx:if="{{item.IsJoin}}">
      <block wx:if="{{item.HasFinish === 1}}">
        <view class="_div data-v-f5666d06 btn daka-btn disabled">已完成</view>
      </block>
      <block wx:else>
        <view class="_div data-v-f5666d06 btn daka-btn disabled" wx:if="{{item.HasClock}}">已打卡</view>
        <view class="_div data-v-f5666d06 btn daka-btn" wx:else>去打卡</view>
      </block>
    </block>
    <block wx:else>
      <view class="_div data-v-f5666d06 btn daka-btn">去打卡</view>
    </block>
  </view>
</template>